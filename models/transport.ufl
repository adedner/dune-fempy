space = Space(2, 1)
u, v = TrialFunction(space), TestFunction(space)
x = SpatialCoordinate(space.cell())
diffusion = NamedCoefficient(space, "diffusion")

velocitySpace = Space(2, 2)
velocity = NamedCoefficient(velocitySpace, "velocity")

a = (inner(grad(u), outer(v, velocity)) + 0.01 * inner(u, v) + diffusion[0] * inner(grad(u), grad(v))) * dx
L = 10 / (1 + dot(x, x)**4) * v[0] * dx(0)

F = a - L
