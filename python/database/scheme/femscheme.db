{
  "FemScheme" :
  {
    "type"      : "FemScheme< $(space), DiffusionModel< typename $(space)::GridPartType, $(space)::dimRange, typename $(space)::RangeFieldType >, $(storage) >",
    "translate" : { "storage" : [ "Adaptive=fem", "Istl=istl" ] },
    "include"   : [ "dune/fem/schemes/femscheme.hh" ]
  },
  "DGFemScheme" :
  {
    "type"      : "DGFemScheme< $(space), DiffusionModel< $(gridpart), $(dimRange) > , $(solver) >",
    "checks"    : [ "'$(solver)' == 'fem' or '$(solver)' == 'femoem' or '$(solver)' == 'istl' or '$(solver)' == 'umfpack' or '$(solver)' == 'petsc'" ], 
    "include"   : [ "dune/fem/schemes/dgfemscheme.hh" ]
  }
}
