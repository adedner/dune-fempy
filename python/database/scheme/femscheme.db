    "type"      : "FemScheme< $(space), DiffusionModel< typename $(space)::GridPartType, $(space)::dimRange, typename $(space)::RangeFieldType >, DifferentiableEllipticOperator, $(storage) >",
    "translate" : { "storage" : [ "Adaptive=fem", "Istl=istl" ] },
    "include"   : [ "dune/fem/schemes/elliptic.hh", "dune/fem/schemes/femscheme.hh" ]
  },
  "DGFemScheme" :
  {
    "type"      : "FemScheme< $(space), DiffusionModel< typename $(space)::GridPartType, $(space)::dimRange, typename $(space)::RangeFieldType >, DifferentiableDGEllipticOperator, $(storage) >",
    "translate" : { "storage" : [ "Adaptive=fem", "Istl=istl" ] },
    "include"   : [ "dune/fem/schemes/dgelliptic.hh", "dune/fem/schemes/femscheme.hh" ]
  },
  "NVDGFemScheme" :
  {
    "type"      : "FemScheme< $(space), DiffusionModel< typename $(space)::GridPartType, $(space)::dimRange, typename $(space)::RangeFieldType >, DifferentiableNVDGEllipticOperator, $(storage) >",
    "translate" : { "storage" : [ "Adaptive=fem", "Istl=istl" ] },
    "include"   : [ "dune/fem/schemes/nvdgelliptic.hh", "dune/fem/schemes/femscheme.hh" ]
  }
