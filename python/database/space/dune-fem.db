{
  "Lagrange" :
  {
    "type"      : "Dune::Fem::LagrangeDiscreteFunctionSpace< Dune::Fem::FunctionSpace< double, double, $(gridpart)::dimensionworld, $(dimrange) >, $(gridpart), $(polorder) >",
    "default"   : [ "dimrange=1", "polorder=1" ],
    "checks"    : [ "$(dimrange) >= 1", "$(polorder) >= 1" ],
    "include"   : [ "dune/fem/space/lagrange.hh" ]
  },
  "DGONB" :
  {
    "type"      : "Dune::Fem::DiscontinuousGalerkinSpace< Dune::Fem::FunctionSpace< double, double, $(gridpart)::dimensionworld, $(dimrange) >, $(gridpart), $(polorder) >",
    "default"   : [ "dimrange=1", "polorder=1" ],
    "checks"    : [ "$(dimrange) >= 1", "$(polorder) >= 0" ],
    "include"   : [ "dune/fem/space/discontinuousgalerkin.hh" ]
  },
  "P1Bubble" :
  {
    "type"      : "Dune::Fem::BubbleElementSpace< Dune::Fem::FunctionSpace< double, double, $(gridpart)::dimensionworld, $(dimrange) >, $(gridpart) >",
    "default"   : [ "dimrange=$(gridpart)::dimensionworld" ],
    "checks"    : [ "$(dimrange) >= 1" ],
    "include"   : [ "dune/fem/space/p1bubble.hh" ]
  }
}
